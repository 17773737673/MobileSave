<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:example="http://schemas.android.com/apk/res/com.example.reviewmobile"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#e74c3c"
    android:orientation="vertical" >

    <com.sdf.jazzylistviewdemo.romainpiel.shimmer.ShimmerTextView
        example:reflectionColor="@color/black"
        style="@style/title"
        android:text="设置中心" />

    <com.example.reviewmobile.view.SettingView
        android:id="@+id/sv_updata"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        example:desc_off="自动更新已关闭"
        example:desc_on="自动更新已开启"
        example:title="自动更新设置" >
    </com.example.reviewmobile.view.SettingView>

    <com.example.reviewmobile.view.SettingView
        android:id="@+id/sv_location"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        example:desc_off="归属地设置已关闭"
        example:desc_on="归属地设置已开启"
        example:title="电话归属地设置" >
    </com.example.reviewmobile.view.SettingView>
    
    <com.example.reviewmobile.view.SettingView
        android:id="@+id/sv_blacknumber"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        example:desc_off="黑名单设置已关闭"
        example:desc_on="黑名单设置已开启"
        example:title="黑名单拦截设置" >
    </com.example.reviewmobile.view.SettingView>

    <com.example.reviewmobile.view.SettingAddressStyle
        android:id="@+id/sas_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
    </com.example.reviewmobile.view.SettingAddressStyle>

    <com.example.reviewmobile.view.SettingAddressStyle
        android:id="@+id/sas_location"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
    </com.example.reviewmobile.view.SettingAddressStyle>

</LinearLayout>